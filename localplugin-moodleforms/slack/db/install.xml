<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/slack/db" VERSION="20200131" COMMENT="XMLDB file for Moodle plugin local/slack"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
<TABLES>    
	<TABLE NAME="userdata" COMMENT="Default comment for userdata">
        <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
            <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            <FIELD NAME="fullname" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false"/>
		    <FIELD NAME="password" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
            <FIELD NAME="email" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false"/>
		    <FIELD NAME="age" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
			<FIELD NAME="saveskills" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>			
            <FIELD NAME="profession" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>			
            <FIELD NAME="introexp" TYPE="char" LENGTH="1333" NOTNULL="true" SEQUENCE="false"/>
            <FIELD NAME="dob" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>            
            <FIELD NAME="userstartdate" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>            
            <FIELD NAME="userenddate" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            <FIELD NAME="userpic" TYPE="char" LENGTH="1333" NOTNULL="true" SEQUENCE="false"/>			
            <FIELD NAME="certificate" TYPE="char" LENGTH="1333" NOTNULL="true" SEQUENCE="false"/>			
            <FIELD NAME="multimedia" TYPE="char" LENGTH="1333" NOTNULL="true" SEQUENCE="false"/>			
            <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
            <FIELD NAME="descriptionformat" TYPE="int" LENGTH="2" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/> 			
		</FIELDS>
        <KEYS>
            <KEY NAME="primary" TYPE="primary" FIELDS="id"/>		
        </KEYS>
    </TABLE>
</TABLES>  
</XMLDB>